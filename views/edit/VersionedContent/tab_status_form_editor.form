<?xml version="1.0" encoding="UTF-8"?>

<form>
  <title></title>
  <name>tab_status_form</name>
  <action></action>
  <enctype></enctype>
  <method></method>
  <i18n_domain>silva</i18n_domain>
  <i18n>tab_status_form_editor</i18n>

  <groups multiple="true">
    <group>
      <title>default</title>
      <fields multiple="true">

      <field><id>publish_datetime</id> <type>DateTimeField</type>
        <values>
          <date_only type="int">0</date_only>
          <alternate_name></alternate_name>
          <input_style>text</input_style>
          <hidden type="int">0</hidden>
          <input_order>dmy</input_order>
          <time_separator>:</time_separator>
          <date_separator>/</date_separator>
          <external_validator></external_validator>
          <required type="int">1</required>
          <default_now type="int">1</default_now>
          <css_class></css_class>
          <title>publish time</title>
          <description>day / month / year &amp;nbsp;(23:59)</description>
          <allow_empty_time type="int">1</allow_empty_time>
        </values>
        <tales>
          <time_separator>python:form.get_time_punctuation()</time_separator>
          <date_separator>python:form.get_date_punctuation()</date_separator>
          <default>python:field.REQUEST.model.get_next_version_publication_datetime()</default> 
        </tales>
      </field>
      <field><id>expiration_datetime</id> <type>DateTimeField</type>
        <values>
          <date_only type="int">0</date_only>
          <alternate_name></alternate_name>
          <input_style>text</input_style>
          <input_order>dmy</input_order>
          <hidden type="int">0</hidden>
          <time_separator>:</time_separator>
          <date_separator>/</date_separator>
          <external_validator></external_validator>
          <required type="int">0</required>
          <default_now type="int">0</default_now>
          <css_class></css_class>
          <title>expiration time</title>
          <description>Empty indicates no expiration.</description>
          <allow_empty_time type="int">1</allow_empty_time>
        </values>
        <tales>
          <time_separator>python:form.get_time_punctuation()</time_separator>
          <date_separator>python:form.get_date_punctuation()</date_separator>
          <default>python:field.REQUEST.model.get_next_version_expiration_datetime()</default> 
        </tales>
      </field>
      <field><id>publish_now_flag</id> <type>CheckBoxField</type>
        <values>
          <alternate_name></alternate_name>
          <hidden type="int">0</hidden>
          <css_class></css_class>
          <default type="int">0</default>
          <title>publish now</title>
          <extra></extra>
          <description></description>
          <external_validator></external_validator>
        </values>
        <tales>
        </tales>
      </field>
      <field><id>message</id> <type>RawTextAreaField</type>
        <values>
          <alternate_name></alternate_name>
          <hidden type="int">0</hidden>
          <max_length></max_length>
          <width type="int">65</width>
          <external_validator></external_validator>
          <height type="int">7</height>
          <required type="int">0</required>
          <css_class></css_class>
          <default></default>
          <title>message</title>
          <truncate type="int">0</truncate>
          <description></description>
          <extra>wrap="soft"</extra>
          <unicode>1</unicode>
        </values>
        <tales>
        </tales>
      </field>
      </fields>
    </group>
  </groups>
</form>
