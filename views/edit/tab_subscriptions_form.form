<?xml version="1.0"?>

<form>
  <title>subscription Form</title>
  <row_length>4</row_length>
  <name>subscriptionform</name>
  <action></action>
  <method>POST</method>
  <enctype></enctype>
  <encoding>UTF-8</encoding>
  <stored_encoding>ISO-8859-1</stored_encoding>
  <unicode_mode>true</unicode_mode>
  <i18n_domain>silva</i18n_domain>
  <i18n></i18n>
  <groups>
    <group>
      <title>Default</title>
      <fields>
        <field><id>subscribable</id> <type>ListField</type>
          <values>
            <alternate_name></alternate_name>
            <css_class>store</css_class>
            <default></default>
            <description>
              Set the subscribability for this content (Yes or No), or set it
              to acquire the subscribability from above (the default).
            </description>
            <enabled type="int">1</enabled>
            <external_validator></external_validator>
            <extra>id="subscribable"</extra>
            <extra_item></extra_item>
            <first_item type="int">1</first_item>
            <hidden type="int">0</hidden>
            <items type="list">[(u'Acquire setting from above', u'2'), (u'Yes', u'1'), (u'No', u'0')]</items>            <required type="int">1</required>
            <size type="int">5</size>
            <title>subscribable</title>
            <unicode type="int">0</unicode>
            <whitespace_preserve type="int">0</whitespace_preserve>
          </values>
          <tales>
            <default>python: str(request.subscribable.subscribability())</default>
          </tales>
          <messages>
            <message name="external_validator_failed">The input failed the external validator.</message>
            <message name="required_not_found">Input is required but no input given.</message>
            <message name="unknown_selection">You selected an item that was not in the list.</message>
          </messages>
        </field>
        <field><id>subscriptions</id> <type>EmailLinesField</type>
          <values>
            <alternate_name></alternate_name>
            <css_class>store</css_class>
            <default type="list">[]</default>
            <description>
              List of email addresses (one per line) currently subscribed to this content.
            </description>
            <enabled type="int">1</enabled>
            <external_validator></external_validator>
            <extra>id="subscriptions"</extra>
            <height type="int">8</height>
            <hidden type="int">0</hidden>
            <max_length></max_length>
            <max_linelength></max_linelength>
            <max_lines></max_lines>
            <required type="int">0</required>
            <title>subscribed email addresses</title>
            <unicode type="int">0</unicode>
            <view_separator><br /></view_separator>
            <whitespace_preserve type="int">0</whitespace_preserve>
            <width type="int">40</width>
          </values>
          <tales>
            <default>python: request.subscriptions</default>
            <height>python: max(5, 2 + len(request.subscriptions))</height>
          </tales>
          <messages>
            <message name="external_validator_failed">The input failed the external validator.</message>
            <message name="required_not_found">Input is required but no input given.</message>
            <message name="too_many_lines">You entered too many lines.</message>
            <message name="line_too_long">A line was too long.</message>
            <message name="too_long">You entered too many characters.</message>
          </messages>
        </field>
      </fields>
    </group>
  </groups>
</form>